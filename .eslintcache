"55"],"import React, { useState } from 'react'\r\nimport './FuncState.css'\r\n\r\nexport default function FuncState() {\r\n    const [state,setState] = useState({conso: 0})\r\n    const [so,demSo] = useState(0);\r\n    const [count,setCount] = useState(0);\r\n    const [hien,setHien] = useState(true);\r\n    const [show,setShow] = useState(true);\r\n    const [check,setCheck] = useState(true);\r\n\r\n    const handleNumber = () => {\r\n        setState({\r\n            conso: state.conso + 1\r\n        })\r\n    }\r\n\r\n    const tangSo = () => {\r\n        demSo(so + 1);\r\n    }\r\n\r\n    const giamSo = () => {\r\n        demSo(so - 1);\r\n    }\r\n\r\n    const handleCount = (isAdd) => {\r\n        if(isAdd) {\r\n            setCount(count + 1);\r\n        } else {\r\n            setCount(count - 1);\r\n        }\r\n    }\r\n\r\n    const sukienClick = () => {\r\n        setHien(false)\r\n    }\r\n\r\n    const handleClick = () => {\r\n        setShow(false)\r\n    }\r\n\r\n    const handleToggle = () => {\r\n        setCheck(!check)\r\n    }\r\n\r\n\r\n    return (\r\n        <div className=\"funcstate-component\">\r\n            <div className=\"container\">\r\n            <h3>Giá trị {so}</h3>\r\n                <button className=\"btn btn-dark mr-3\" onClick={tangSo}>Tăng</button>\r\n                <button className=\"btn btn-dark\" onClick={giamSo}>Giảm</button>\r\n\r\n                <br></br>\r\n                <br></br>\r\n\r\n                <h3>Giá trị {so}</h3>\r\n                <button className=\"btn btn-success mr-3\" onClick={tangSo}>Tăng</button>\r\n                <button className=\"btn btn-danger\" onClick={giamSo}>Giảm</button>\r\n\r\n                <br></br>\r\n                <br></br>\r\n\r\n                <h3>Number {count}</h3>\r\n                <button className=\"btn btn-primary mr-3\" onClick={ () => {handleCount(true)} }>Minus</button>\r\n                <button className=\"btn btn-warning\" onClick={ () => {handleCount(false)} }>Plus</button>\r\n\r\n                <br></br>\r\n                <br></br>\r\n\r\n                <h3 className={hien ? 'hien' : 'an'}>Hello Everyone !</h3>\r\n                <button className=\"btn btn-info\" onClick={sukienClick}>Ẩn chữ</button>\r\n\r\n                <br></br>\r\n                <br></br>\r\n\r\n                {show && <h3>Hello Everyone !</h3> }\r\n                <button className=\"btn btn-dark\" onClick={handleClick}>Hide text</button>\r\n\r\n                <br></br>\r\n                <br></br>\r\n                \r\n                <div>\r\n                    {check && <h3 className=\"\">Hello Everone !</h3>}\r\n                    <button className=\"btn mr-3\" style={{backgroundColor: \"#bf360c\"}} onClick={handleToggle}>Toggle text</button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n","C:\\Users\\Hong Hong\\Desktop\\React_HH\\src\\ReactClass\\ClassDatabinding\\ClassDatabinding.js",[],"C:\\Users\\Hong Hong\\Desktop\\React_HH\\src\\ReactClass\\ClassRedux\\ClassMapStateToProps\\ClassMapStateToProps.js",[],"C:\\Users\\Hong Hong\\Desktop\\React_HH\\src\\App.js",["56","57","58","59","60","61","62","63"],"C:\\Users\\Hong Hong\\Desktop\\React_HH\\src\\redux\\SanPhamReducer.js",[],"C:\\Users\\Hong Hong\\Desktop\\React_HH\\src\\ReactFunction\\FuncRedux\\FuncUseSelector\\FuncUseSelector.js",[],"C:\\Users\\Hong Hong\\Desktop\\React_HH\\src\\redux\\SanPhamFunctionReducer.js",[],"C:\\Users\\Hong Hong\\Desktop\\React_HH\\src\\ReactClass\\ClassRedux\\ClassMapDispatchToProps\\ClassMapDispatchToProps.js",["64"],"C:\\Users\\Hong Hong\\Desktop\\React_HH\\src\\redux\\GioHangReducer.js",[],{"ruleId":"65","severity":1,"message":"66","line":12,"column":11,"nodeType":"67","messageId":"68","endLine":12,"endColumn":23},{"ruleId":"65","severity":1,"message":"69","line":2,"column":8,"nodeType":"67","messageId":"68","endLine":2,"endColumn":24},{"ruleId":"65","severity":1,"message":"70","line":3,"column":8,"nodeType":"67